package com.mindtree.patient.vo;



public class DoctorDetails {
	
	
	private int id;
	
	private String docName;
	
	private int docAge;
	
	private String gender;
	
	private String specialistIn;
	
	private int noOfPatients;
	
	public DoctorDetails() {
		super();
	}



	



	public DoctorDetails(int id, String docName, int docAge, String gender, String specialistIn, int noOfPatients) {
		super();
		this.id = id;
		this.docName = docName;
		this.docAge = docAge;
		this.gender = gender;
		this.specialistIn = specialistIn;
		this.noOfPatients = noOfPatients;
	}







	public int getId() {
		return id;
	}



	public void setId(int id) {
		this.id = id;
	}



	public String getDocName() {
		return docName;
	}



	public void setDocName(String docName) {
		this.docName = docName;
	}



	public int getDocAge() {
		return docAge;
	}



	public void setDocAge(int docAge) {
		this.docAge = docAge;
	}



	public String getSpecialistIn() {
		return specialistIn;
	}



	public void setSpecialistIn(String specialistIn) {
		this.specialistIn = specialistIn;
	}



	public int getNoOfPatients() {
		return noOfPatients;
	}



	public void setNoOfPatients(int noOfPatients) {
		this.noOfPatients = noOfPatients;
	}



	public String getGender() {
		return gender;
	}







	public void setGender(String gender) {
		this.gender = gender;
	}







	@Override
	public String toString() {
		return "DoctorDetails [id=" + id + ", docName=" + docName + ", docAge=" + docAge + ", specialistIn="
				+ specialistIn + ", noOfPatients=" + noOfPatients + "]";
	}
	
	
	
	

}
